{
  "name": "glitch-deploy-branch",
  "version": "0.1.0",
  "scripts": {
    "start": "node server.js",
    "build": "bsb -make-world && webpack",
    "build:production": "bsb -make-world && NODE_ENV=production webpack",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"bsb -make-world -w\" \"webpack -w\" \"yarn server\"",
    "clean": "bsb -clean-world",
    "publish-branch": "node publish.js"
  },
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "engines": {
    "node": "10.x"
  },
  "author": "Feihong Hsu",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@glennsl/bs-json": "^2.0.0",
    "@jsiebern/bs-material-ui": "^0.3.1",
    "@material-ui/core": "^1.3.0",
    "bs-platform": "^3.1.5",
    "gh-pages": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "reason-react": ">=0.4.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
